{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true,
        "commonjs": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:vue/essential",
        "prettier",
        "@vue/prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2020,
        "sourceType": "module",
        "parser": "@babel/eslint-parser"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-unused-vars": "error",
        "no-inner-declarations": "error",
        "vue/no-mutating-props": "off"
        // "eqeqeq": "error"
    }
}